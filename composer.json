{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-iconv": "*",
        "aws/aws-sdk-php": "^3.87",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": ">= 1.0",
        "doctrine/doctrine-bundle": "^2.7.0",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2|^3",
        "gedmo/doctrine-extensions": "^3.8",
        "knplabs/knp-paginator-bundle": "^5",
        "knplabs/knp-time-bundle": "^1.8",
        "league/flysystem-async-aws-s3": "^3",
        "league/flysystem-aws-s3-v3": ">= 1.0",
        "league/flysystem-bundle": "^2.3",
        "league/flysystem": "^2",
        "liip/imagine-bundle": ">= 2.1",
        "michelf/php-markdown": "^1.9",
        "oneup/flysystem-bundle": "^4.4",
        "php-http/guzzle6-adapter": ">= 1.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.6",
        "sensio/framework-extra-bundle": "^5.1 || ^6.0",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "^5.4|^6.0",
        "symfony/console": "^5.4|^6.0",
        "symfony/flex": "^1.9",
        "symfony/form": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/property-access": "^5.4|^6.0",
        "symfony/property-info": "^5.4|^6.0",
        "symfony/runtime": "^5.4|^6.0",
        "symfony/security-bundle": "^5.4|^6.0",
        "symfony/serializer": "^5.4|^6.0",
        "symfony/twig-bundle": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0",
        "twig/extra-bundle": "^3.4",
        "twig/markdown-extra": "^3.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3",
        "phpstan/phpstan": "^1.8",
        "rector/rector": "^0.13.9",
        "symfony/debug-bundle": "^3.3|^5.0",
        "symfony/dotenv": "^5.4|^6.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.3|^5.0",
        "symfony/stopwatch": "^5.4|^6.0",
        "symfony/var-dumper": "^3.3|^5.0",
        "symfony/web-profiler-bundle": "^5.4|^6.0",
        "zenstruck/foundry": "^1.21"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {},
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
            "allow-contrib": true,
            "symfony": {
                "require": "^6.0"
            }
        }
}
